- if ( have_rows('flex_editor') ) :;
    - while ( have_rows('flex_editor') ) : the_row();
        - flexContent()
        - flexContentAndImage()
        - flexAccordion()
        - flexTabs()
        - flexVideoEmbed()
        - flexTeam()
        - flexSlider()
        - flexMultidirectionalSlider()
        - flexAtAGlanceSlider()
        - flexLargeImageSlider()
        - flexTestimonialSlider()
        - flexFeaturedContentSlider()
        - flexTestimonialCards()
        - flexHomeHero()
        - flexContentTwoCol()
        - flexGalleryGrid()
        - flexGalleryCarousel()
        - flexDarkCTA()
        - flexHomeServices()
    - endwhile
- endif

- function flexContent() {
    - if ( 'content_block' === get_row_layout() ) :;
        - get_template_part( 'includes/flex/flex', 'content');
    - endif;
- }

